{
  "name": "@fitnet/backend-boilerplate",
  "version": "1.0.22",
  "bin": "./scripts/generate.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fitness-Network/backend-boil.git"
  },
  "description": "Package for authorization with keycloak",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --runInBand",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up:infra": "docker compose -f docker/infra.yaml --env-file .env up -d",
    "module": "node ./scripts/new-module/index.js"
  },
  "dependencies": {
    "@fitnet/backend-boilerplate": "^1.0.21",
    "@huggingface/inference": "^3.5.2",
    "@huggingface/transformers": "^3.4.0",
    "@keycloak/keycloak-admin-client": "^25.0.4",
    "@langchain/google-genai": "^0.0.2",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^7.4.0",
    "@types/passport-local": "^1.0.38",
    "axios": "^1.7.5",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "cache-manager-redis-yet": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "commander": "^12.1.0",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "google-auth-library": "^9.14.0",
    "handlebars": "^4.7.8",
    "keycloak-connect": "^25.0.4",
    "langchain": "^0.3.19",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "^8.5.2",
    "mongoose-autopopulate": "^1.1.0",
    "nest-commander": "^3.15.0",
    "nest-keycloak-connect": "^1.10.1",
    "nestjs-telegraf": "^2.8.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.14",
    "openai": "^4.87.3",
    "openid-client": "^5.6.5",
    "otp-generator": "^4.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-telegram": "^1.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "superagent": "^10.1.0",
    "telegraf": "^4.16.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.15",
    "@types/otp-generator": "^4.0.2",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "bugs": {
    "url": "https://github.com/Fitness-Network/backend-boil/issues"
  },
  "homepage": "https://github.com/Fitness-Network/backend-boil#readme",
  "main": "generate.js",
  "directories": {
    "test": "test"
  },
  "overrides": {
    "@langchain/core": "0.1.5"
  }
}
